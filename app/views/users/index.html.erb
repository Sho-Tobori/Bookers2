  <div class="container">
    <div class="row">
      <div class="col-xs-3">
        <%= render 'books/list'%>
      </div>
        <div class="col-xs-9">
        <h1>Users</h1>
        <table class="table">
          <tr>
            <th>Image</th>
            <th>name</th>
            <th></th>
          </tr>
          <tr>
            <% @users.each do |user| %>
            <td><%= attachment_image_tag user, :profile_image, :fill, 70, 70, format: 'jpeg', fallback: "no_image.jpg" %></td>
            <td><%= user.name %></td>
            <td><%= link_to("Show",user_path(user)) %></td>
          </tr>
           <% end %>
        </table>
    </div>
  </div>
</div>